<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.libraryuserapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LibraryUserApp">
        <activity android:name=".User.About"></activity>
        <activity android:name=".User.EBook" />
        <activity android:name=".User.ViewData" />
        <activity android:name=".User.AllCategories" />
        <activity android:name=".DeleteUser" />
        <activity android:name=".ViewUsers" />
        <activity android:name=".UploadPapers" />
        <activity android:name=".UploadData" />
        <activity android:name=".PasswordReset" />
        <activity android:name=".AdminDashboard" />
        <activity android:name=".User.UserDashboard" />
        <activity android:name=".CreateAccount" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>